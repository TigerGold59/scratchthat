{
  "extends": "@tsconfig/svelte",
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "allowJs": false,
    "checkJs": false,
    "sourceMap": true,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "target": "ESNext",
    "module": "ESNext",
    "paths": {
      "@svelte-app/*": [
        "src/*"
      ]
    },
    "lib": [
      "ES2021",
      "DOM"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}